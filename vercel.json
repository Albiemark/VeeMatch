{
  "buildCommand": "next build",
  "installCommand": "npm install",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "regions": ["iad1"],
  "rewrites": [
    { "source": "/_next/(.*)", "destination": "/_next/$1" },
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/login", "destination": "/login" },
    { "source": "/sign-in/(.*)", "destination": "/sign-in/$1" },
    { "source": "/sign-up/(.*)", "destination": "/sign-up/$1" },
    { "source": "/dashboard", "destination": "/dashboard" },
    { "source": "/create-profile", "destination": "/create-profile" },
    { "source": "/phone-login", "destination": "/phone-login" },
    { "source": "/sso-callback", "destination": "/sso-callback" },
    { "source": "/logout", "destination": "/logout" },
    { "source": "/android-chrome-(.*).png", "destination": "/android-chrome-$1.png" },
    { "source": "/favicon.ico", "destination": "/favicon.ico" },
    { "source": "/manifest.json", "destination": "/manifest.json" },
    { "source": "/globals.css", "destination": "/globals.css" },
    { "source": "/", "destination": "/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
